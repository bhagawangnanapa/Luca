        <sccode name="execute" init_power_mode="0"><![CDATA[i2cStart();
i2cTx(ACC_I2C_ADDR_W);         /////Slave address with write option
if(state.i2cStatus == 0x0000){
i2cTx(CTRL_REG0);
i2cTx(0x10);
i2cStop();
}else{
output.valueX=10;
i2cStop();
}
fwDelayInstrCycles(50);


i2cStart();
i2cTx(ACC_I2C_ADDR_W);
i2cTx(CTRL_REG0);
if(state.i2cStatus == 0x0000) {
    i2cRepeatedStart();
    i2cTx(ACC_I2C_ADDR_R);
    i2cRxAck(output.valueX);
    i2cStop();
}else{
    i2cStop();
}
fwScheduleTask(1);]]></sccode>
