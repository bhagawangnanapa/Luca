 {
	 document.getElementById('trckframe').src='http://hy1.m4dc.com/track/xclick?m=1&s='+getQueryVariable("s")+'&d='+getQueryVariable("m")+'&dsip=60.243.166.56&dsid=395845&DCAS=DCAS-HY-HYD';
	 
	 parent_obj.postMessage('close','http://'+getQueryVariable('d'));
