<script>
	window.dataLayer = window.dataLayer || [];
	function gtag(){dataLayer.push(arguments);}
	gtag('js', new Date());

	gtag('config', 'UA-119042418-6');
  
	function gtag_trackEvent(evnt_cat, evnt_act, evnt_lbl, evnt_val) {
		if (evnt_cat == '' || typeof evnt_cat == 'undefined') {	evnt_cat = 'na'; }
		if (evnt_act == '' || typeof evnt_act == 'undefined') {	evnt_act = 'na'; }
		if (evnt_lbl == '' || typeof evnt_lbl == 'undefined'||evnt_lbl==null) {	evnt_lbl = 'na'; }
		if (evnt_val == '' || typeof evnt_val == 'undefined'||evnt_val==null) {	evnt_val = 1; }

		gtag('event', evnt_act, {
		  'event_category': evnt_cat,
		  'event_label': evnt_lbl,
		  'value': evnt_val
		});
	}  
</script>


