<script>

 var refer_url ='http://localhost:55432/';
 var parent_obj=window.top;
 
if(refer_url=="http://www.airtel.in/dot/")
	parent_obj=window.parent;

var flag=true;

function getQueryVariable(variable) {
  var query = window.location.search.substring(1);
  var vars = query.split("&");
  for (var i=0;i<vars.length;i++) {
    var pair = vars[i].split("=");
    if (pair[0] == variable) {
      return pair[1];
    }
  } 
 }
 
 function open_popup(e)
 { 
	 var o=900,t=700;
	 try{screen.width>0&&(o=Math.round(.9*screen.width),t=Math.round(.73*screen.height))}catch(n){};
	 var i="",r=screen.width/2-o/2,a=screen.height/2-t/2;
	 window.open(e,i,"toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=yes, resizable=no, copyhistory=no, width="+o+", height="+t+", top="+a+", left="+r);
 } 
 
 function mtrck()
 {  
 
		var dynamic_script = document.createElement( 'script' );		
		dynamic_script.setAttribute( 'src', 'http://hy1.m4dc.com/i/mtrck.jsp?m=1&s='+getQueryVariable("s")+'&d='+getQueryVariable("m") +'&dsip=60.243.166.56&dsid=395845&DCAS=DCAS-HY-HYD' );
		
		var body_obj = document.getElementsByTagName('body')[0];
		body_obj.appendChild( dynamic_script );
		
		//document.getElementById('trckframe').src='http://hy1.m4dc.com/i/mtrck.jsp?m=1&s='+getQueryVariable("s")+'&d='+getQueryVariable("m") +'&dsip=60.243.166.56&dsid=395845&DCAS=DCAS-HY-HYD';	
 }
 
 function ctrck(e)
 { 

	var  event_target=e.target ;
	var  page_ttile =document.title;	
	if(typeof event_target == 'undefined'){ event_target='';}
	if(typeof page_ttile == 'undefined'){ page_ttile='';}	
	gtag_trackEvent(page_ttile,'Button-Click',event_target,1);
	
	if(typeof msg_log_pkid != 'undefined')				
		document.getElementById('ctrckframe').src='http://hy1.m4dc.com/i/ctrck.jsp?m=1&s='+getQueryVariable("s")+'&d='+getQueryVariable("m") +"&pkid="+msg_log_pkid+'&dsip=60.243.166.56&dsid=395845&DCAS=DCAS-HY-HYD';
	else
		document.getElementById('ctrckframe').src='http://hy1.m4dc.com/i/ctrck.jsp?m=1&s='+getQueryVariable("s")+'&d='+getQueryVariable("m") +"&pkid="+0 +'&dsip=60.243.166.56&dsid=395845&DCAS=DCAS-HY-HYD';

 }
 
