function checkheight(height_param)
{
	
	var ahd=0;
	var dhd=h;
	
	if(document.getElementById('headb'))
	{
		ahd+=document.getElementById('headb').offsetHeight;
		dhd-=document.getElementById('headb').offsetHeight;
	}

	if(document.getElementById('id_sec_div'))
	{
		ahd+=document.getElementById('id_sec_div').offsetHeight;
		dhd-=document.getElementById('id_sec_div').offsetHeight;
	}

	if(document.getElementById('id_third_div'))
	{
		ahd+=document.getElementById('id_third_div').offsetHeight;
		dhd-=document.getElementById('id_third_div').offsetHeight;
		
	}	
	
	if(document.getElementById('bot_div'))
	{
		ahd+=document.getElementById('bot_div').offsetHeight;
		dhd-=document.getElementById('bot_div').offsetHeight;
		
	}	 
	
	 if(document.getElementById('msgcnt'))
	 ahd+=document.getElementById('msgcnt').offsetHeight;
 
 //if(document.getElementById('msgcnt').offsetHeight>height_param)

 if(ahd>0 && ahd>h && dhd>0)	 
 {
	 if(document.getElementById('msgcnt'))
	 {
		 height_param=dhd;
     }
	  	 document.getElementById('msgcnt').style.height= (height_param+1)+"px";
	 
	 if(getQueryVariable("dt")!=2)
	 {
		document.getElementById('msgcnt').style.overflow="hidden";
	 
		if (document.getElementById('msgcnt').addEventListener) {
		document.getElementById('msgcnt').addEventListener('mouseover', setOverflow, false);
		} else { // IE
		document.getElementById('msgcnt').attachEvent('onmouseover', setOverflow);
		}  
		
		if (document.getElementById('msgcnt').addEventListener) {
		document.getElementById('msgcnt').addEventListener('mouseout', hideOverflow, false);
		} else { // IE
		document.getElementById('msgcnt').attachEvent('onmouseout', hideOverflow);
		} 
	 }
	 else
		 document.getElementById('msgcnt').style.overflow="auto";
		 
 }
 
} 

function checkOverflow()
{
	 if(getQueryVariable("dt")!=2)
	 {
		document.getElementById('cnt').style.overflow="hidden";
	 
		if (document.getElementById('cnt').addEventListener) {
		document.getElementById('cnt').addEventListener('mouseover', setOverflow, false);
		} else { // IE
		document.getElementById('cnt').attachEvent('onmouseover', setOverflow);
		}  
		
		if (document.getElementById('cnt').addEventListener) {
		document.getElementById('cnt').addEventListener('mouseout', hideOverflow, false);
		} else { // IE
		document.getElementById('cnt').attachEvent('onmouseout', hideOverflow);
		} 
		
		
		if(document.getElementById("inner_confirm"))
        {			
			document.getElementById("inner_confirm").style.overflow="hidden";
			
			if (document.getElementById('inner_confirm').addEventListener) {
			document.getElementById('inner_confirm').addEventListener('mouseover', setInnerOverflow, false);
			} else { // IE
			document.getElementById('inner_confirm').attachEvent('onmouseover', setInnerOverflow);
			}  

			if (document.getElementById('inner_confirm').addEventListener) {
			document.getElementById('inner_confirm').addEventListener('mouseout', hideInnerOverflow, false);
			} else { // IE
			document.getElementById('inner_confirm').attachEvent('onmouseout', hideInnerOverflow);
			} 
		}	
		
		if(document.getElementById("inner_sucess"))
        {			
			document.getElementById("inner_sucess").style.overflow="hidden";
			
			if (document.getElementById('inner_sucess').addEventListener) {
			document.getElementById('inner_sucess').addEventListener('mouseover', setSuccessOverflow, false);
			} else { // IE
			document.getElementById('inner_sucess').attachEvent('onmouseover', setSuccessOverflow);
			}  

			if (document.getElementById('inner_sucess').addEventListener) {
			document.getElementById('inner_sucess').addEventListener('mouseout', hideSuccessOverflow, false);
			} else { // IE
			document.getElementById('inner_sucess').attachEvent('onmouseout', hideSuccessOverflow);
			} 
		}
		
		
	 }
	 else
	 {
		 document.getElementById('cnt').style.overflow="auto";
		 
		 if(document.getElementById("inner_confirm"))
			 document.getElementById('inner_confirm').style.overflow="auto";
		 
		 if(document.getElementById("inner_sucess"))
			 document.getElementById('inner_sucess').style.overflow="auto";
	 }	 
}

function hideInnerOverflow()
{	
	document.getElementById('inner_confirm').style.overflow="hidden"; 
}

function setInnerOverflow()
{		
	document.getElementById('inner_confirm').style.overflowX="hidden"; 
	document.getElementById('inner_confirm').style.overflowY="auto"; 
}


function hideSuccessOverflow()
{	
	document.getElementById('inner_sucess').style.overflow="hidden"; 
}

function setSuccessOverflow()
{	
	document.getElementById('inner_sucess').style.overflowX="hidden"; 
	document.getElementById('inner_sucess').style.overflowY="auto"; 
	
}




function hideOverflow()
{	
	document.getElementById('cnt').style.overflow="hidden"; 
}

function setOverflow()
{	
	document.getElementById('cnt').style.overflowX="hidden"; 
	document.getElementById('cnt').style.overflowY="auto"; 

}

function setheight(height_param)
{
	 
 if(document.getElementById('msgcnt').offsetHeight>height_param)
 {
	 parent_obj.postMessage('setHeight_'+(document.getElementById('msgcnt').offsetHeight+150),'http://'+getQueryVariable('d')); 
 }


}

function vertiAlign(id)
{
	if(document.getElementById(id))
	{ 
		document.getElementById(id).style.top=((h/2)-(document.getElementById(id).offsetHeight/2))+"px";
	}
	 
} 


